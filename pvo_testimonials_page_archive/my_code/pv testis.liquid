    testimonialModalCard,
    ".modal-video"
  );
  const testimonialModalIframe = bringFrom(testimonialModalVideoBox, "iframe");

  //----------elements: tutorial modal
  const tutorialModalContainer = bring("#tutorial-modal");
  const tutorialModalCard = bringFrom(tutorialModalContainer, ".modal-card");
  const tutorialModalIframe = bringFrom(tutorialModalCard, "iframe");

  //----------fill elements and add event listeners
  landingSocialPhotos.forEach((photo, i) => {
    const info = photosInfo[i];
    photo.dataset.src = `url('${info["thumbnail"]}')`;
    photo["aria-label"] = info["aria-label"];
    photo.addEventListener("click", () => {
      openModal(i, "SOCIAL");
    });
  });
  landingTestimonialsCards.forEach((card, i) => {
    const info = testimonialsInfo[i];
    const media = bringFrom(card, ".card-thumbnail");
    media["aria-label"] = info["aria-label"];
    media.dataset.src = `url('${info["thumbnail"]}')`;
    bringFrom(card, ".card-testimonial__comment").innerHTML =
      info.typography.comment;
    bringFrom(card, ".card-namecard__name").innerHTML = info.typography.name;
    bringFrom(card, ".card-namecard__occupation").innerHTML =
      info.typography.occupation;
    bringFrom(card, ".card__media-container").addEventListener("click", () => {
      openModal(i, "TESTIMONIAL");
    });
  });
  landingTutorialsCards.forEach((card, i) => {
    const info = tutorialsInfo[i];
    card.dataset.src = "url('" + info["thumbnail"] + "')";
    card.style["background-size"] = "100% 100%";
    card["aria-label"] = info["aria-label"];
    card.addEventListener("click", () => {
      openModal(i, "TUTORIAL");
    });
  });

  ////////////////////////////////////////////////////////////////////
  //////////////////FILL, OPEN, and CLOSE MODAL///////////////////////
  ////////////////////////////////////////////////////////////////////

  //----------fill modal
  const fillSocialModalCard = (i) => {
    photoCurrentPostId = photosInfo[i].postId;
    instagramModalIframe.src = instagramModalIframe.src.replace(
      photoPlaceholderPostId,
      photoCurrentPostId
    );
    instagramModalIframe.onload = () => {
      if (!instagramModalIframe.src.includes(photoPlaceholderPostId)) {
        removeClassFrom(instagramModalCard, "lazy-load__modal");
      }
    };
  };
  const fillTestimonialModalCard = (i) => {
    const mediaSrc = testimonialsInfo[i]["embed-link"];
    const testimonial = testimonialsInfo[i]["typography"]["comment"];
    const name = testimonialsInfo[i]["typography"]["name"];
    const occupation = testimonialsInfo[i]["typography"]["occupation"];

    testimonialModalIframe.src = mediaSrc;
    bringFrom(
      testimonialModalContainer,
      ".modal-testimonial__comment"
    ).innerHTML = testimonial;
    bringFrom(testimonialModalContainer, ".modal-namecard__name").innerHTML =
      name;
    bringFrom(
      testimonialModalContainer,
      ".modal-namecard__occupation"
    ).innerHTML = occupation;
  };
  const fillTutorialModalCard = (i) => {
    tutorialModalIframe.src = tutorialsInfo[i]["embed-link"];
  };

  //----------open modal
  const openModal = async (i, modalType) => {
    addClassToAll([modalBackdrop, modalCloseButton], modalType);
    if (modalType == "SOCIAL") {
      if (!instagramModalIframe) {
        console.log("fuck");
        instagramModalIframe = bring("#instagram-embed-0");
      }
      makeDisplayUnset(instagramModalContainer);
      await fillSocialModalCard(i);
      setTimeout(() => {
        addStyleTo(instagramModalContainer, "opacity", 1);
      }, 500);
    } else if (modalType == "TESTIMONIAL") {
      addClassTo(testimonialModalVideoBox, "lazy-load__modal");
      makeDisplayUnset(testimonialModalContainer);
      await fillTestimonialModalCard(i);
      setTimeout(() => {
        addStyleTo(testimonialModalContainer, "opacity", 1);
      }, 500);
      makeDisplayNone(testimonialModalIframe);

      testimonialModalIframe.addEventListener("load", () => {
        removeClassFrom(testimonialModalVideoBox, "lazy-load__modal");
        makeDisplayUnset(testimonialModalIframe);
      });
    } else {
      //modalType=="TUTORIAL"
      addClassTo(tutorialModalCard, "lazy-load__modal");
      makeDisplayUnset(tutorialModalContainer);
      await fillTutorialModalCard(i);
      setTimeout(() => {
        addStyleTo(tutorialModalContainer, "opacity", 1);
      }, 500);
      makeDisplayNone(tutorialModalIframe);

      tutorialModalIframe.addEventListener("load", () => {
        removeClassFrom(tutorialModalCard, "lazy-load__modal");
        makeDisplayUnset(tutorialModalIframe);
      });
    }
    makeDisplayUnset(modalContainer);
    setTimeout(() => {
      addStyleTo(modalContainer, "opacity", 1);
    }, 0);
    addStyleTo(body, "overflow-y", "hidden");
    [modalBackdrop, modalCloseButton].forEach((el) => {
      el.addEventListener("click", (e) => {
        closeModal(modalType);
      });
    });
  };

  //----------close modal
  const closeModal = (modalType) => {
    if (modalType == "SOCIAL") {
      container = instagramModalContainer;
      instagramModalIframe.src = instagramModalIframe.src.replace(
        photoCurrentPostId,
        photoPlaceholderPostId
      );
    } else if (modalType == "TESTIMONIAL") {
      container = testimonialModalContainer;
      testimonialModalIframe.src = "";
    } else {
      //modalType=="TUTORIAL"
      container = tutorialModalContainer;
      tutorialModalIframe.src = "";
    }
    addStyleTo(container, "opacity", 0);
    setTimeout(() => {
      makeDisplayNone(container);
    }, 500);
    addStyleTo(modalContainer, "opacity", 0);
    setTimeout(() => {
      makeDisplayNone(modalContainer);
    }, 500);
    addStyleTo(body, "overflow-y", "scroll");
    [modalBackdrop, modalCloseButton].forEach((el) => {
      removeClassFrom(el, modalType);
    });
  };

  ////////////////////////////////////////////////////////////////////
  ////////////////////////INVIEW ANIMATION////////////////////////////
  ///////////////////partly substituted by lozad//////////////////////
  ////////////////////////////////////////////////////////////////////

  //----------measure highlight x and y
  const measureHighlighting = () => {
    const socialRect = landingSocialBody.getBoundingClientRect();
    // const boxWidth = socialRect.right - socialRect.left;
    // const boxHeight = socialRect.bottom - socialRect.top;
    const boxWidth = socialRect.width;
    const boxHeight = socialRect.height;

    landingSocialPhotos.forEach((from, i) => {
      const fromRect = from.getBoundingClientRect();
      const fromWidth =
        (fromRect.right - fromRect.left) * photosInfo[i].highlighting[i];
      const fromHeight =
        (fromRect.bottom - fromRect.top) * photosInfo[i].highlighting[i];
      const fromDiagonal = Math.sqrt(
        Math.pow(fromWidth, 2) + Math.pow(fromHeight, 2)
      );
      landingSocialPhotos.forEach((to, j) => {
        if (i != j) {
          const toRect = to.getBoundingClientRect();
          const toWidth =
            (toRect.right - toRect.left) * photosInfo[j].highlighting[i][2];
          const toHeight =
            (toRect.bottom - toRect.top) * photosInfo[j].highlighting[i][2];
          const toDiagonal = Math.sqrt(
            Math.pow(toWidth, 2) + Math.pow(toHeight, 2)
          );
          const a = Math.floor(
            (toRect.left + toRect.right) / 2 -
              (fromRect.left + fromRect.right) / 2
          );
          const b = Math.floor(
            (toRect.top + toRect.bottom) / 2 -
              (fromRect.top + fromRect.bottom) / 2
          );
          const c = Math.floor(
            Math.sqrt(Math.pow(Math.abs(a), 2) + Math.pow(Math.abs(b), 2))
          );
          if (c < fromDiagonal) {
            photosInfo[i].highlighting[j] = [0.1 * a, 0.1 * b, 0.98];
          } else if (c < fromDiagonal * 2) {
            photosInfo[i].highlighting[j] = [
              (0.1 * a) / 2,
              (0.1 * b) / 2,
              0.95,
            ];
          } else if (c < fromDiagonal * 3) {
            photosInfo[i].highlighting[j] = [
              (0.05 * a) / 3,
              (0.05 * b) / 3,
              0.92,
            ];
          } else {
            photosInfo[i].highlighting[j] = [
              (0.025 * a) / 4,
              (0.025 * b) / 4,
              0.89,
            ];
          }
          // console.log(from, to, a, b, c, photosInfo[i].highlighting[j]);
        }
      });
    });
  };

  //----------function to apply animation to specific photo
  const animateByDistance = (i, j) => {
    if (i != j) {
      const from = landingSocialPhotos[i];
      const to = landingSocialPhotos[j];
      to.style.transform =
        "translate(" +
        photosInfo[i].highlighting[j][0] +
        "px, " +
        photosInfo[i].highlighting[j][1] +
        "px) scale(" +
        photosInfo[i].highlighting[j][2] +
        ")";
    }
  };

  //----------apply animation to all photos
  const applyHighlighting = () => {
    landingSocialPhotos.forEach((from, i) => {
      if (from.style.animation) {
        from.style.animation = "unset";
        from.style.opacity = 1;
      }
      setTimeout(() => {
        from.addEventListener(
          "mouseenter",
          (mouseEnterEvent = () => {
            from.style.transform =
              "scale(" + photosInfo[i].highlighting[i] + ")";
            landingSocialPhotos.forEach((to, j) => {
              animateByDistance(i, j);
            });
          })
        );
        from.addEventListener(
          "mouseleave",
          (mouseLeaveEvent = () => {
            landingSocialPhotos.forEach((all) => {
              all.style.transform = "unset";
            });
          })
        );
      }, 0);
    });
    const removeMouseoverEvent = () => {
      if (window.innerWidth <= 750 && isInView(landingSocialBody)) {
        landingSocialPhotos.forEach((photo) => {
          photo.removeEventListener("mouseenter", mouseEnterEvent);
          photo.removeEventListener("mouseleave", mouseLeaveEvent);
        });
      }
    };
    window.addEventListener("resize", removeMouseoverEvent);
    window.addEventListener("orirentationChange", removeMouseoverEvent);
  };

  //----------events that should be listened to for isInView check
  const inViewEvents = ["load", "scroll", "resize", "orientationChange"];

  //----------check if an element is in viewport
  const isInView = (el) => {
    const wTop = window.scrollY || window.pageYOffset;
    const wBottom = wTop + window.innerHeight;
    const elTop = wTop + el.getBoundingClientRect().top;
    const elBottom = wTop + el.getBoundingClientRect().bottom;
    if (elBottom < wTop || elTop > wBottom) {
      // console.log(
      //   "for ",
      //   el,
      //   ", ",
      //   elBottom,
      //   " is less than ",
      //   wTop,
      //   " or ",
      //   elTop,
      //   " is greater than ",
      //   wBottom
      // );
      return false;
    } else return true;
  };
  const windowEventRemover = (listener) => {
    console.log("<<<<" + listener + " for window has been removed.>>>>");
    inViewEvents.forEach((e) => window.removeEventListener(e, listener));
  };

  //----------seperately apply listener to achieve better performance
  const socialAnimationFlags = [0, 0, 0, 0, 0, 0, 0, 0];
  const socialAnimationListener = () => {
    if (window.innerWidth > 750 && isInView(landingSocialBody)) {
      landingSocialPhotos.forEach((photo, i) => {
        addAnimationTo(photo, "growFtm 1s forwards");
        addStyleTo(photo, "animation-delay", photosInfo[i]["animation-delay"]);
      });
      measureHighlighting();
      setTimeout(() => {
        applyHighlighting();
        windowEventRemover(socialAnimationListener);
      }, 2000);
    } else if (window.innerWidth <= 750) {
      if (socialAnimationFlags.reduce((sum, flag) => (sum += flag), 0) == 8)
        windowEventRemover(socialAnimationListener);
      landingSocialPhotos.forEach((photo, i) => {
        if (isInView(photo)) {
          addAnimationTo(photo, "growFtm 1s forwards");
          socialAnimationFlags[i] = 1;
        }
      });
      const inCaseOfResizing = () => {
        if (window.innerWidth > 750 && isInView(landingSocialBody))
          applyHighlighting();
      };
      landingSocialBody.addEventListener("resize", inCaseOfResizing);
      landingSocialBody.addEventListener(
        "orirentationChange",
        inCaseOfResizing
      );
    }
  };
  const testimonialsAnimationListener = () => {
    if (isInView(landingTestimonialsBody)) {
      landingTestimonialsCards.forEach((card, i) => {
        addAnimationTo(card, "growFtm 1s forwards");
        addStyleTo(card, "animation-delay", i * 100 + "ms");
      });
      windowEventRemover(testimonialsAnimationListener);
    }
  };
  const tutorialsAnimationFlags = [0, 0, 0, 0, 0, 0];
  const tutorialsAnimationListener = () => {
    if (tutorialsAnimationFlags.reduce((sum, flag) => (sum += flag), 0) == 6)
      windowEventRemover(tutorialsAnimationListener);
    landingTutorialsCards.forEach((card, i) => {
      if (isInView(card)) {
        addAnimationTo(card, "appear-upward 1s forwards");
        tutorialsAnimationFlags[i] = 1;
      }
    });
  };

  //----------apply in-view animations to window element
  inViewEvents.forEach((e) =>
    window.addEventListener(e, socialAnimationListener)
  );
  inViewEvents.forEach((e) =>
    window.addEventListener(e, testimonialsAnimationListener)
  );
  inViewEvents.forEach((e) =>
    window.addEventListener(e, tutorialsAnimationListener)
  );

  window.addEventListener("click", (e) => {
    console.log(e.target);
  });
</script>

  //////////////////////////CUSTOM FUNCTIONS//////////////////////////
  ////////////////////////////////////////////////////////////////////
  const bring = (selector) => document.querySelector(selector);
  const bringAll = (selector) => document.querySelectorAll(selector);
  const bringFrom = (element, selector) => element.querySelector(selector);
  const bringAllFrom = (element, selector) =>
    element.querySelectorAll(selector);

  const addClassTo = (element, className) => {
    if (!element.classList) element.class = className;
    else if (!element.classList.contains(className))
      element.classList.add(className);
    else console.log(className + " hasn't been added to ", element);
  };
  const addClassToAll = (elements, className) => {
    elements.forEach((element) => addClassTo(element, className));
  };
  const removeClassFrom = (element, className) => {
    if (element.classList && element.classList.contains(className)) {
      element.classList.remove(className);
      console.log(className + " has been removed from ", element);
    }
  };

  const classIsIn = (element, className) => {
    if (element.classList)
      return Array.from(element.classList).includes(className);
    else return false;
  };
  const classIsInAll = (elements, className) => {
    for (let el of elements) {
      if (classIsIn(el, className)) continue;
      return false;
    }
    return true;
  };

  const addStyleTo = (element, styleName, styleValue) =>
    (element.style[styleName] = styleValue);
  const addStyleToAll = (elements, styleName, styleValue) =>
    elements.forEach((el) => (el.style[styleName] = styleValue));

  const addAnimationTo = (element, animationDescription) => {
    element.style.animation = animationDescription;
  };
  const addAnimationToAll = (elements, animationDescription) => {
    elements.forEach((element) => {
      element.style.animation = animationDescription;
    });
  };

  const makeDisplayNone = (element) => {
    if (classIsIn(element, "display-unset"))
      removeClassFrom(element, "display-unset");
    else addClassTo(element, "display-none");
  };
  const makeDisplayUnset = (element) => {
    if (classIsIn(element, "display-none"))
      removeClassFrom(element, "display-none");
    else addClassTo(element, "display-unset");
  };

  const getHeight = (element) => {
    const rect = element.getBoundingClientRect();
    return rect.height;
  };
  const getWidth = (element) => {
    const rect = element.getBoundingClientRect();
    return rect.width;

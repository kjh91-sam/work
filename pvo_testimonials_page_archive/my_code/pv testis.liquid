  /* STYLE PRESETS */
  .flex-centered {
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
  }
  .display-none {
    display: none !important;
  }
  .display-unset {
    display: unset !important;
  }
  .lazy-load__modal {
    animation: loading-modal 1.7s infinite linear;
    background: linear-gradient(
      to right,
      #eeeeee 10%,
      #dddddd 18%,
      #eeeeee 33%
    );
    background-size: 100% 100%;
  }

  /* COMMON STYLING */
  .master-container a,
  .master-container button {
    text-decoration: none;
    border: none;
    cursor: pointer;
  }
  /* .master-container button:hover {
    transform: scale(0.9);
  } */
  .master-container {
    width: 100vw;
    overflow-x: hidden;
    position: relative;
  }

  /* ANIMATIONS */
  @keyframes loading-modal {
    from {
      background-position: 0 0;
    }
    to {
      background-position: 774px 0;
    }
  }
  @keyframes fade-in {
    0% {
      opacity: 0%;
    }
    100% {
      opacity: 100%;
    }
  }
  @keyframes fade-out {
    0% {
      opacity: 100%;
    }
    100% {
      opacity: 0%;
    }
  }
  @keyframes floating {
    0% {
      transform: translate(0, 0);
    }
    50% {
      transform: translate(0, 20px);
    }
    100% {
      transform: translate(0, 0px);
    }
  }
  @keyframes appear-upward {
    0% {
      transform: translateY(10%);
      opacity: 0%;
    }
    100% {
      transform: translate(0);
      opacity: 100%;
    }
  }
  @keyframes appear-downward {
    0% {
      transform: translateY(-10%);
      opacity: 0%;
    }
    100% {
      transform: translate(0);
      opacity: 100%;
    }
  }
  @keyframes rotate {
    0% {
      transform: rotate(0deg);
    }
    100% {
      transform: rotate(360deg);
    }
  }
  @keyframes growFtm {
    from {
      transform: scale(0);
      opacity: 0;
    }
    to {
      transform: scale(1);
      opacity: 1;
    }
  }
  @keyframes attract {
    0% {
      transform: translateX(0) translateY(0);
    }
    17% {
      transform: translateX(3px) translateY(3px);
    }
    34% {
      transform: translateX(0) translateY(0);
    }
    51% {
      transform: translateX(3px) translateY(3px);
    }
    68% {
      transform: translateX(0) translateY(0);
    }
  }
